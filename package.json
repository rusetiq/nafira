{
  "name": "nafira-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "cd server && node server.js",
    "build": "npm run build:frontend && npm run install:server",
    "build:frontend": "react-scripts build",
    "install:server": "cd server && npm install --omit=dev",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "cd server && npm run dev",
    "server:init": "cd server && npm run init-db",
    "install:all": "npm install && cd server && npm install",
    "dev": "concurrently \"npm start\" \"npm run server\"",
    "dev:start": "react-scripts start"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@react-three/fiber": "^8.15.12",
    "@react-three/postprocessing": "^2.16.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.4",
    "gsap": "^3.14.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "lucide-react": "^0.294.0",
    "playwright": "^1.57.0",
    "postprocessing": "^6.34.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.8.0",
    "three": "^0.160.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}